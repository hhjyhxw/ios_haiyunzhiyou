<view><view class="tab tab-fixed"><view class="tab-nav"><block wx:if="{{status=='UNCOMPLETED'}}"><view class="tab-title active">全部订单</view></block><block wx:else><view data-event-opts="{{[['tap',[['getOderList',['UNCOMPLETED']]]]]}}" class="tab-title" bindtap="__e">全部订单</view></block><block wx:if="{{status=='UNPAID'}}"><view class="tab-title active">待付款</view></block><block wx:else><view data-event-opts="{{[['tap',[['getOderList',['UNPAID']]]]]}}" class="tab-title" bindtap="__e">待付款</view></block><block wx:if="{{status=='UNSHIPPED'}}"><view class="tab-title active">待收货</view></block><block wx:else><view data-event-opts="{{[['tap',[['getOderList',['UNSHIPPED']]]]]}}" class="tab-title" bindtap="__e">待收货</view></block><block wx:if="{{status=='COMPLETED'}}"><view class="tab-title active">已完成</view></block><block wx:else><view data-event-opts="{{[['tap',[['getOderList',['COMPLETED']]]]]}}" class="tab-title" bindtap="__e">已完成</view></block></view></view><view class="tab-main"><view class="tab-cont active"><block wx:for="{{orderlist}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="order"><view class="item"><view class="order-hd"><text class="shop">{{item.supplierName}}</text><text class="state">{{item.tradeStatusName}}</text></view></view><view data-event-opts="{{[['tap',[['toOrderitem',['$0'],[[['orderlist','',index,'id']]]]]]]}}" class="item" bindtap="__e"><view class="order-bd"><view class="order-info"><view><image class="order-img" src="../../static/logo.png" mode></image></view><view class="order-num"><text class="order-num-text">共1件</text></view></view></view></view><view class="order-ft"><view class="time">{{item.createDate}}</view><view class="money">{{"￥"+item.totalAmount}}</view><block wx:if="{{item.tradeStatus=='unprocessed'&&item.paymentStatus=='unpaid'}}"><view class="go2pay"><text>去支付</text></view></block></view></view></block></view></view><view class="container999"><tab-bar vue-id="1" currentPage="{{currentPage}}" bind:__l="__l"></tab-bar></view></view>