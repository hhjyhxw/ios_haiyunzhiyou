<view class="page"><view class="header1"><view data-event-opts="{{[['tap',[['getLocationInfo',['$event']]]]]}}" class="lbs-btn" bindtap="__e"><text>{{address}}</text></view></view><view class="allshop"><block wx:for="{{supplierList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toShopDetail',['$0'],[[['supplierList','',index,'id']]]]]]]}}" class="shoplist" bindtap="__e"><view class="shop_img"><image class="shop_img_in" src="{{item.supplierImg}}"></image></view><view class="shop_info"><view class="shop_name">{{item.storeName}}</view><view class="shop_busnesstime">{{"服务时间："+item.businessHours}}</view><view class="shop_address">{{"地址:"+item.address}}</view><view class="shop_tel">{{"电话:"+item.contactPhone}}</view><view class="shop_distance">{{item.disatance+"m"}}</view></view><view class="shop_go"></view></view></block></view><view class="container999"><tab-bar vue-id="1" currentPage="{{currentPage}}" bind:__l="__l"></tab-bar></view></view>