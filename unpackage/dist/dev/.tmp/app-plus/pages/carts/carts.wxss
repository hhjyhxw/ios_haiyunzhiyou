.active {
  background-color: #E13F3F;
}
.footer {
  position: absolute;
  z-index: 999;
  bottom: 0;
  left: 0;
  background: #3A424A;
  width: 100%;
  min-width: 320px;
  /* height: 4.5rem; */
  color: white;
  font-size: 25px;
  line-height: 25px;
}
.footer .menus {
  padding: 0;
  margin: 0 auto;
  list-style: none;
  width: 100%;
}
.menus .menu {
  font-size: 0.6rem;
  float: left;
  width: 25%;
  /* height: 4.5rem; */
  text-align: center;
  padding: 0.2rem  0 0 0;
  box-sizing: border-box;
  margin: 0 auto;
  -webkit-tap-highlight-color: transparent;
}
.menus .menu .menu-icon {
  display: block;
  width: 100%;
  height: 1.3rem;
  background-size: 20px 20px;
  color: #ffffff;
}
.menus .menu .menu-icon1 {
  background: url(data:image/png;base64,) center center no-repeat scroll;
  background-size: 23px;
